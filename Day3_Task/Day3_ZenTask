1. For the given JSON iterate over all for loops (for, for in, for of, forEach)

var sampleobj=[{ person: "Name-1", age: "2", company: "GUVI" },{ person: "Name-2", age: "5", company: "GUVI geek" },
                  { person: "Name-3", age: "8", company: "GUVI geek network" }]

//for 
console.log("For loop Example:")
for(var obj=0; obj<sampleobj.length;obj++)
{
    console.log("Person:"+" "+sampleobj[obj].person)
    console.log("Age:"+" "+sampleobj[obj].age)
    console.log("Company:"+" "+sampleobj[obj].company)
}
console.log("____________________________________________")
//for in
console.log("For-In  loop Example:")
for(var obj in sampleobj)
{
    console.log("Person:"+" "+sampleobj[obj].person)
    console.log("Age:"+" "+sampleobj[obj].age)
    console.log("Company:"+" "+sampleobj[obj].company)
}
console.log("____________________________________________")
//for of
console.log("For-Of  loop Example:")
for(var obj of sampleobj)
{
    console.log("Person:"+" "+obj.person)
    console.log("Age:"+" "+obj.age)
    console.log("Company:"+" "+obj.company)
}
console.log("____________________________________________")
//forEach
console.log("For-Each  loop Example:")
sampleobj.forEach((arrayItem) =>{
   console.log("Person:"+" "+arrayItem.person)
   console.log("Age:"+" "+arrayItem.age)
   console.log("Company:"+" "+arrayItem.company)
});
console.log("____________________________________________")

